using System;


class SequencesOfEqualStrings
{
    static void Main()
    {
        string inputString = Console.ReadLine();
        string[] items = inputString.Split(' ');

        for (int i = 0; i < items.Length - 1; i++)
        {
            Console.Write(items[i] + " ");
            if (items[i] != items[i + 1])
            {
                Console.WriteLine();
            }
        }
        Console.Write(items[items.Length - 1]);
        Console.WriteLine();

        




    }
}

